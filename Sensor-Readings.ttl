@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix unit: <http://qudt.org/vocab/unit/> .

<https://piveau.ac3-project.eu/datasets/uc2-sensor-readings>
    a dcat:Dataset ;
    dct:title "UC2 Sensor Readings"@en ;
    dct:description "Dataset containing sensor readings including temperature, humidity, CO, and CO2 levels."@en ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/ENVI> ;
    dct:issued "2024-07-18T00:00:00"^^xsd:dateTime ;
    dct:modified "2024-07-18T00:00:00"^^xsd:dateTime ;
    dct:language <http://publications.europa.eu/resource/authority/language/ENG> ;
    dct:provenance [ a dct:ProvenanceStatement ;
                      rdfs:label "Sensor readings collected from sensors deployed in UC2 test site."@en ] ;
    dcat:keyword "sensor readings", "temperature", "humidity", "CO", "CO2" ;
    dcat:contactPoint [ a vcard:Kind ;
                        vcard:hasEmail <mailto:contact@ac3-project.eu> ;
                        vcard:hasName "AC3 Project Team" ;
                        vcard:fn "AC3 Project" ] ;
    dcat:distribution [ a dcat:Distribution ;
                        dct:format <http://publications.europa.eu/resource/authority/file-type/CSV> ;
                        dcat:accessURL <https://ac3-project.eu/data/uc2/sensor-readings.csv> ;
                        dct:license <https://creativecommons.org/licenses/by/4.0/> ;
                        dct:title "Sensor Readings CSV (Available Soon)" ;
                        dct:description "CSV file containing sensor readings. Data will be generated and available soon." ;
                        sosa:observes quantitykind:CelsiusTemperature, quantitykind:RelativeHumidity, quantitykind:CarbonMonoxideConcentration, quantitykind:CO2Equivalent ;
                        ssn:hasMeasurementUnit unit:DEG_C, unit:PERCENT_RH, unit:PPM, unit:PPM ] .
